					<dataContractSerializer maxItemsInObjectGraph="2147483647" />
					<serviceTimeouts transactionTimeout="00:30:00" />
				</behavior>
			</serviceBehaviors>
		</behaviors>
		<bindings>
			<webHttpBinding>
				<binding name="mainBindingConfig" closeTimeout="00:30:00" openTimeout="00:30:00"
				 receiveTimeout="00:30:00" sendTimeout="00:30:00" maxBufferSize="2147483647"
				 maxBufferPoolSize="2147483647" maxReceivedMessageSize="2147483647">
					<readerQuotas maxDepth="2147483647" maxStringContentLength="2147483647"
					 maxArrayLength="2147483647" maxBytesPerRead="2147483647" maxNameTableCharCount="2147483647" />
					<security mode="None">
						<transport proxyCredentialType="None" />
					</security>
				</binding>
			</webHttpBinding>
		</bindings>
		<serviceHostingEnvironment aspNetCompatibilityEnabled="true" multipleSiteBindingsEnabled="true"/>
		<standardEndpoints>
			<webHttpEndpoint>
				<standardEndpoint name="mainEndpoint" maxBufferSize="2147483647" faultExceptionEnabled="True"
				 maxBufferPoolSize="2147483647" maxReceivedMessageSize="2147483647"
				 transferMode="Streamed" helpEnabled="true" defaultOutgoingResponseFormat="Json"
				 automaticFormatSelectionEnabled="true">
					<readerQuotas maxDepth="2147483647" maxStringContentLength="2147483647"
					 maxArrayLength="2147483647" maxBytesPerRead="2147483647" maxNameTableCharCount="2147483647" />
				</standardEndpoint>
			</webHttpEndpoint>
		</standardEndpoints>
	</system.serviceModel>

