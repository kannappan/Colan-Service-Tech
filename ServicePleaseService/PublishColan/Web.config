				</host>
			</service>
		</services>
		<behaviors>
			<endpointBehaviors>
				<behavior name="mainEndpointBehavior">
					<webHttp helpEnabled="true" defaultOutgoingResponseFormat="Json" defaultBodyStyle="Bare"
					 automaticFormatSelectionEnabled="true" faultExceptionEnabled="true" />
					<dataContractSerializer maxItemsInObjectGraph="2147483647" />
				</behavior>
			</endpointBehaviors>
			<serviceBehaviors>
				<behavior name="serviceTechSvcBehavior">
          <serviceDebug includeExceptionDetailInFaults="true" />
